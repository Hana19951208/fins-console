import request from '@/utils/request'

export const getOrgTree = data => {
  return request( {
    url: '/sysMktOrgan/getOrgTree',
    method: 'get',
    data
  } )
}

export const saveOrg = data => {
  return request( {
    url: '/sysMktOrgan/save',
    method: 'post',
    data
  } )
}

export const updateOrg = data => {
  return request( {
    url: '/sysMktOrgan/update',
    method: 'post',
    data
  } )
}

export const deleteOrg = data => {
  return request( {
    url: '/sysMktOrgan/delete',
    method: 'post',
    data
  } )
}